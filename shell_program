then
let jum2=$argo2*2000;
fi
let total=jum+jum2;

zenity --info --title "GO TRAIN" --text "Anda akan membeli tiket kereta api dengan format sebagai berikut :
Kota Asal\t\t\t\t\t=  Surabaya
Stasiun Asal\t\t\t\t=  Gubeng Surabaya
