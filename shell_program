function Surabaya_Pasar_Turi() {
supatu=$(zenity --width 700 --height 500 --list --title "GO TRAIN" --text "Pilih data berikut" --checklist --column=" " --column="Jenis Kereta" --column="Nama Kereta" --column="Jam Berangkat" --column="Jam Tiba" --column="Harga per orang" false Eksekutif_J Argo_Bromo 08.00 11.20 Rp.365.000,00 false Eksekutif_I Sembrani 09.00 12.52 Rp.390.000,00 false Eksekutif_A Harina 16.30 20.38 Rp.400.000,00 false Bussiness_B Gumarang 15.30 19.38 Rp.370.000,00 false Bussiness_B_ Harina 16.30 20.38 Rp.320.000,00 false Bussiness_N Harina 16.30 20.38 Rp.280.000,00 false Economy_C Maharani 06.00 10.28 Rp.49.000,00 false Economy_C_ Kertajaya 23.35 03.57 Rp.100.000,00 false Economy__C Ambarawa 13.10 17.16 Rp.90.000,00)
case $supatu in
"Eksekutif_J") argo_bromo;;
"Eksekutif_I") sembrani;;
"Eksekutif_A") harina3;;
"Bussiness_B") gumarang;;
"Bussiness_B_") harina5;;
"Bussiness_N") harina6;;
"Economy_C") maharani;;
"Economy_C_") kertajaya;;
"Economy__C") ambarawa;;
esac
}
