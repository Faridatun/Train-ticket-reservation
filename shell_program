#=============================================================
function Semarang() {
stasiun=$(zenity --title="GO TRAIN" --width 500 --height 300 --list --text="Pilih stasiun keberangkatan dan tujuan anda" --radiolist --column="Check" --column="Stasiun Asal" --column="Stasiun Tujuan" false Surabaya_Pasar_Turi Semarang_Tawang false Surabaya_Gubeng Semarang_Tawang_ false Surabaya_Pasar_Turi_ Semarang_Ponco false Surabaya_Gubeng_ Semarang_Ponco_ ); echo $smrang
case $stasiun in
"Surabaya_Pasar_Turi") Surabaya_Pasar_Turi ;;
"Surabaya_Gubeng") Surabaya_Gubeng ;;
"Surabaya_Pasar_Turi_") Surabaya_Pasar_Turi_ ;;
"Surabaya_Gubeng_") Surabaya_Gubeng_ ;;
esac
}
function Yogyakarta() {
stasiun=$(zenity --title="GO TRAIN" --width 500 --height 300 --list --text="Pilih stasiun keberangkatan dan tujuan anda" --radiolist --column="Check" --column="Stasiun Asal" --column="Stasiun Tujuan" false Surabaya__Gubeng Yogyakarta_Lempuyangan false Surabaya__Gubeng_ Stasiun_Yogyakarta false Stasiun_Wonokromo Yogyakarta_Lempuyangan ); echo $ygy
case $stasiun in
"Surabaya__Gubeng") Surabaya__Gubeng ;;
"Surabaya__Gubeng_") Surabaya__Gubeng_ ;;
"Stasiun_Wonokromo") Stasiun_Wonokromo ;;
esac
}
function Bandung() {
stasiun=$(zenity --title="GO TRAIN" --width 500 --height 300 --list --text="Pilih stasiun keberangkatan dan tujuan anda" --radiolist --column="Check" --column="Stasiun Asal" --column="Stasiun Tujuan" false _Surabaya_Gubeng _Stasiun_Bandung false _Surabaya_Pasar_Turi _Stasiun_Bandung);
case $stasiun in
"_Surabaya_Gubeng") _Surabaya_Gubeng ;;
"_Surabaya_Pasar_Turi") _Surabaya_Pasar_Turi ;;
esac
}
function Jakarta() {
stasiun=$(zenity --title="GO TRAIN" --width 500 --height 300 --list --text="Pilih stasiun keberangkatan dan tujuan anda" --radiolist --column="Check" --column="Stasiun Asal" --column="Stasiun Tujuan" false _Surabaya_Gubeng_ Pasar_Senen false _Surabaya_Pasar_Turi_ Stasiun_Gambir false Stasiun_Wonokromo_ Stasiun_Jatinegara);
case $stasiun in
"_Surabaya_Gubeng_") _Surabaya_Gubeng_ ;;
"_Surabaya_Pasar_Turi_") _Surabaya_Pasar_Turi_ ;;
"Stasiun_Wonokromo_") Stasiun_Wonokromo_ ;;
esac
}
function destination() {
tujuan=$(zenity --title="GO TRAIN" --width 400 --height 300 --list --text="Pilih kota tujuan anda" --radiolist --column="Check" --column="Kota Tujuan" false Semarang false Yogyakarta false Bandung false Jakarta); 
case $tujuan in
"Semarang") Semarang ;;
"Yogyakarta") Yogyakarta ;;
"Bandung") Bandung ;;
"Jakarta") Jakarta ;;
esac
}
cetak(){
echo "====DATA PEMESAN TIKET KERETA API===="
echo "========Asal Kota Surabaya==========="
echo "Nama			= $nm"
echo "No. ID (KTP/SIM)	= $id"
echo "No. Telp		= $telpon"
echo "E-mail			= $email"
echo "Tujuan			= $tujuan"
echo "Stasiun Keberangkatan	= $stasiun"
echo "Tanggal Keberangkatan	= $tgl"
echo "Menu Kereta Pilihan	= $supatu"
echo "Jumlah Penumpang	= $argo"
echo "Kargo Tambahan		= $argo2"
echo "Biaya Penumpang		= $jum"
echo "Biaya Kargo		= $jum2"
echo "Total Biaya		= $total"
echo "====================================="
menu;
}

menu(){
men=`zenity --width 500 --height 400 --title "GO-TRAIN : Pemesanan Tiket Kereta APi" --list --text "Menu Pilihan" --radiolist --column="" --column "Menu" false "Pemesanan Tiket" false "Pencetakan Tiket" false "Data Pemesan Tiket" false "Hapus Data Pemesan"`
case $men in
"Pemesanan Tiket") pesan;;
"Pencetakan Tiket") cetak;;
"Data Pemesan Tiket") daftar;;
"Hapus Data Pemesan") hapus;;
esac
}
menu;
if [ "$?" == 0 ]
then
ask=`zenity --question --text "Apakah anda yakin ingin keluar?" --title "Keluar" --ok-label="Ya" --cancel-label="Tidak"`
if [ "$?" == 0 ]
then
zenity --info --title "Sampai jumpa lagi" --text "Terima kasih telah menggunakan aplikasi ini"
exit 0;
else menu;
fi
fi
