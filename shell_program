#!/bin/bash
zenity --info --width 400 --height 200 --text "Program Pemesanan Tiket Kereta Api (Kota Asal Surabaya)\nFaridatun Ni'mah\n1210161017\n2 D4 Telekomunikasi A\nPoliteknik Elektronika Negeri Surabaya"
loading(){
(
   echo "25"; sleep 1
   echo "#Loading . . . .";
   echo "50"; sleep 1
   echo "#Loading . . . .";
   echo "75"; sleep 1
   echo "#Loading . . . .";
   echo "100"; sleep 1
   echo "#Finish . . . ."; sleep 1
) | zenity --progress --cancel-label "exit" --width 400 --height 100 \
--title="GO-TRAIN : Pemesanan Tiket Kereta Api" \
--text="Loading...." \
--percentage=10 --timeout=2
if [ $? -eq 1 ]; then
exit
fi
}
loading;
set_id(){
ida=`awk 'BEGIN{i=1s}
{
printf"ids="i";";
i++;
}'<"dataku21.txt"`;
eval "$ida";
}
daftar(){
data=`awk 'BEGIN{FS=":";i=0}
{
print "d["i"]="$1"; d["i+1"]="$2"; d["i+2"]="$3";d["i+3"]="$4"; d["i+4"]="$5"; d["i+5"]="$6"; d["i+6"]="$7";d["i+7"]="$8"; d["i+8"]="$9"; d["i+9"]="$10"; d["i+10"]="$11";"; 
i+=11;
}' < "dataku21.txt"`;
eval "$data"; #LOOP
zenity --list --width 700 --height 500 --title "Daftar Pemesan Tiket Kereta Api" \
--column "No" --column "Nama" --column "No.Id" --column "No. Telepon" --column  "Email" --column "Tujuan" --column "Stasiun" --column "Tanggal" --column "Biaya Tiket" --column "Biaya kargo" --column "Total Biaya"  "${d[@]}";  
menu;
}
hapus(){
zenity --question --text "Anda yakin ingin menghapus data yang paling akhir?";
if [ $? -eq 0 ];
then
banding=`cat dataku21.txt | wc -l`;
echo "$(sed -i "$banding d" dataku21.txt)";
benar=$(zenity --info --title "Berhasil" --text "Data sudah dihapus");
echo "$(bash task8.sh)";
exit
fi
menu;
}
pesan(){
destination;
identitas;
menu;
}
function identitas() {
tgl=$(zenity --calendar --title "GO-TRAIN" --text="Pilih tanggal keberangkatan anda.");
if [ $? -eq 1 ];
then 
echo "Tidak ada tanggal yang dipilih"
menu;
fi
nm=$(zenity --width 400 --height 100 --title="GO TRAIN" --entry --text="Nama lengkap anda : ");
if [ $? -eq 1 ]; then
zenity --width 400 --height 200 --error --text "Belum Terisi. Lengkapi terlebih dahulu untuk melanjutkan proses pemesanan tiket kereta api."
identitas;
fi
id=$(zenity --width 400 --height 100 --title="GO TRAIN" --entry --text="No. Identitas anda (KTP/SIM): ");
if [ $? -eq 1 ]; then
zenity --width 400 --height 200 --error --text "Belum Terisi. Lengkapi terlebih dahulu untuk melanjutkan proses pemesanan tiket kereta api."
identitas;
fi
telpon=$(zenity --width 400 --height 100 --title="GO TRAIN" --entry --text="No.Telp anda : "); 
if [ $? -eq 1 ]; then
zenity --width 400 --height 200 --error --text "Belum Terisi. Lengkapi terlebih dahulu untuk melanjutkan proses pemesanan tiket kereta api."
identitas;
fi
email=$(zenity --width 400 --height 100 --title="GO TRAIN" --entry --text="Email anda : ");
if [ $? -eq 1 ]; then
zenity --width 400 --height 200 --error --text "Belum Terisi. Lengkapi terlebih dahulu untuk melanjutkan proses pemesanan tiket kereta api."
identitas;
fi
bab=$(zenity --title="GO-TRAIN" --info --text="Untuk melengkapi proses pembelian tiket lakukan pembayaran. Pembayaran dapat dilakukan melaui ATM/Internet Banking Bank Mandiri atau Outlet Payment Point Fastpay dengan memasukkan KODE PEMBAYARAN anda. ")
penutup=$(zenity --width 400 --height 100 --title="GO TRAIN" --info --text="Semoga perjalanan anda menyenangkan.")
set_id;
echo "$(($ids+1)):$nm:$id:$telpon:$email:$tujuan:$stasiun:$tgl:$jum:$jum2:$total" >> "dataku21.txt";
daftar;
}
function argo_bromo() {
argo=$(zenity --title "GO TRAIN" --entry --text="Masukkan jumlah penumpang = ")
#operasi hitung
if [ $argo -gt 0 ]
then
let jum=$argo*365000;
fi
argo2=$(zenity --title "GO TRAIN" --entry --text="Masukkan jumlah kargo tambahan yang (maks. 40 kg) = (dalam kg)")
#operasi hitung
if [ $argo2 -gt 0 ]
then
let jum2=$argo2*10000;
fi
let total=jum+jum2;
zenity --info --title "GO TRAIN" --text "Anda akan membeli tiket kereta api dengan format sebagai berikut :
Kota Asal\t\t\t\t\t=  Surabaya
Stasiun Asal\t\t\t\t=  Pasar Turi Surabaya
Kota Tujuan\t\t\t\t=  $tujuan
Stasiun Tujuan\t\t\t=  Semarang Tawang
Jenis Kereta\t\t\t\t=  Eksekutif(J)
Nama Kereta\t\t\t\t=  Argo Bromo
Jam Berangkat\t\t\t=  08.00
Jam Tiba\t\t\t\t\t=  11.20
Jumlah Penumpang\t\t=  $argo
Biaya penumpang\t\t=  $jum
Biaya kargo tambahan\t=  $jum2
Total biaya yang harus dibayar = $total
KODE PEMBAYARAN\t\t= 98564997"
}
